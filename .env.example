# AIO Sandbox Environment Configuration

# Authentication
AUTH_USERNAME=admin
AUTH_PASSWORD=admin123

# GitHub Integration
# Repository format: owner/repo-name (e.g., HashWarlock/erc-8004-ex-phala)
GITHUB_REPO=
# GitHub Personal Access Token (classic) with repo scope
# Create one at: https://github.com/settings/tokens/new?scopes=repo&description=AIO%20Sandbox
GH_TOKEN=

# User Configuration
USER_UID=1000
USER_GID=1000

# Service Ports
GEM_SERVER_PORT=8088
PUBLIC_PORT=8080
BROWSER_REMOTE_DEBUGGING_PORT=9222
WEBSOCKET_PROXY_PORT=6080
VNC_SERVER_PORT=5900
AUTH_BACKEND_PORT=8081
MCP_SERVER_PORT=8089

# Display Configuration
DISPLAY_WIDTH=1920
DISPLAY_HEIGHT=1080
TZ=Asia/Singapore

# Logging
LOG_DIR=/tmp/logs
NGINX_LOG_LEVEL=debug

# Optional: Browser Configuration
BROWSER_USER_AGENT=
BROWSER_EXTRA_ARGS=
HOMEPAGE=

# Optional: Proxy Configuration
PROXY_SERVER=

# Optional: Security
JWT_PUBLIC_KEY=
DNS_OVER_HTTPS_TEMPLATES=
